<launch>
  <arg name="use_sim_time" default="true" />
  <arg name="headless" default="false" />
  <arg name="gui" default="true"/>
  <arg name="verbose" default="true"/>
  <arg name="world_file"  default="$(find naviswarm)/worlds/circuit2.world"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="debug" value="0" />
    <arg name="gui" value="$(arg gui)" />
    <arg name="verbose" value="$(arg verbose)"/>
    <arg name="use_sim_time" value="$(arg use_sim_time)" />
    <arg name="headless" value="$(arg headless)" />
    <arg name="world_name" value="$(arg world_file)" />
  </include>

  <arg name="ns0"        default="turtlebot0"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns0)"/>
    <arg name="y"             value="-2"/>
  </include>

  <arg name="ns1"        default="turtlebot1"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns1)"/>
    <arg name="x"             value="2"/>
  </include>


  <arg name="ns2"        default="turtlebot2"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns2)"/>
    <arg name="x"             value="-2"/>
  </include>


  <arg name="ns3"        default="turtlebot3"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns3)"/>
    <arg name="y"             value="2"/>
  </include>

  <arg name="ns4"        default="turtlebot4"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns4)"/>
    <arg name="y"             value="4"/>
  </include>

  <arg name="ns5"        default="turtlebot5"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns5)"/>
    <arg name="x"             value="5"/>
  </include>

  <arg name="ns6"        default="turtlebot6"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns6)"/>
    <arg name="x"             value="6"/>
  </include>

  <arg name="ns7"        default="turtlebot7"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns7)"/>
    <arg name="x"             value="7"/>
  </include>

  <arg name="ns8"        default="turtlebot8"/>
  <include file="$(find naviswarm)/launch/include/turtlebot_base.launch">
    <arg name="ns"            value="$(arg ns8)"/>
    <arg name="x"             value="8"/>
  </include>

  

</launch>
